{
  "branches": [
    "main"
  ],
  "repositoryUrl": "https://github.com/pandeptwidyaop/tempfile.git",
  "plugins": [
    [
      "@semantic-release/commit-analyzer",
      {
        "preset": "conventionalcommits",
        "releaseRules": [
          {
            "type": "feat",
            "release": "minor"
          },
          {
            "type": "fix",
            "release": "patch"
          },
          {
            "type": "docs",
            "release": "patch"
          },
          {
            "type": "style",
            "release": "patch"
          },
          {
            "type": "refactor",
            "release": "patch"
          },
          {
            "type": "perf",
            "release": "patch"
          },
          {
            "type": "test",
            "release": "patch"
          },
          {
            "type": "build",
            "release": "patch"
          },
          {
            "type": "ci",
            "release": "patch"
          },
          {
            "type": "chore",
            "release": "patch"
          },
          {
            "type": "revert",
            "release": "patch"
          },
          {
            "breaking": true,
            "release": "major"
          }
        ]
      }
    ],
    [
      "@semantic-release/release-notes-generator",
      {
        "preset": "conventionalcommits",
        "presetConfig": {
          "types": [
            {
              "type": "feat",
              "section": "‚ú® Features"
            },
            {
              "type": "fix",
              "section": "üêõ Bug Fixes"
            },
            {
              "type": "docs",
              "section": "üìö Documentation"
            },
            {
              "type": "style",
              "section": "üíÑ Styles"
            },
            {
              "type": "refactor",
              "section": "‚ôªÔ∏è Code Refactoring"
            },
            {
              "type": "perf",
              "section": "‚ö° Performance Improvements"
            },
            {
              "type": "test",
              "section": "‚úÖ Tests"
            },
            {
              "type": "build",
              "section": "üîß Build System"
            },
            {
              "type": "ci",
              "section": "üë∑ CI/CD"
            },
            {
              "type": "chore",
              "section": "üßπ Chores"
            },
            {
              "type": "revert",
              "section": "‚è™ Reverts"
            }
          ]
        }
      }
    ],
    [
      "@semantic-release/changelog",
      {
        "changelogFile": "CHANGELOG.md",
        "changelogTitle": "# üìã Changelog\n\nAll notable changes to this project will be documented in this file.\n\nThe format is based on [Keep a Changelog](https://keepachangelog.com/en/1.0.0/),\nand this project adheres to [Semantic Versioning](https://semver.org/spec/v2.0.0.html).\n"
      }
    ],
    [
      "@semantic-release/github",
      {
        "assets": [
          {
            "path": "release/tempfile-linux-amd64.tar.gz",
            "label": "Linux AMD64 Binary"
          },
          {
            "path": "release/tempfile-linux-arm64.tar.gz",
            "label": "Linux ARM64 Binary"
          },
          {
            "path": "release/tempfile-darwin-amd64.tar.gz",
            "label": "macOS AMD64 Binary"
          },
          {
            "path": "release/tempfile-darwin-arm64.tar.gz",
            "label": "macOS ARM64 Binary (Apple Silicon)"
          },
          {
            "path": "release/tempfile-windows-amd64.zip",
            "label": "Windows AMD64 Binary"
          },
          {
            "path": "release/tempfile-docker.tar.gz",
            "label": "Docker Image"
          },
          {
            "path": "release/checksums.txt",
            "label": "Checksums"
          }
        ],
        "assignees": [],
        "releasedLabels": ["released"]
      }
    ],
    [
      "@semantic-release/git",
      {
        "assets": [
          "CHANGELOG.md",
          "package.json"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ]
  ]
}
